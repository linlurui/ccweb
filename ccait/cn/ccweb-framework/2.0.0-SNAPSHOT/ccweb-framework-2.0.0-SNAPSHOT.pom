<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <groupId>ccait.cn</groupId>
    <artifactId>ccweb-framework</artifactId>
    <packaging>pom</packaging>
    <version>2.0.0-SNAPSHOT</version>
    <description>ccait web framework</description>
    <modules>
        <module>ccweb-api</module>
        <module>ccweb-office</module>
        <module>ccweb-auth</module>
        <module>ccweb-iot</module>
        <module>ccweb-config</module>
        <module>ccweb-logs</module>
        <module>ccweb-socket</module>
        <module>ccweb-webagent</module>
        <module>ccweb-start</module>
        <module>ccweb-repo</module>
        <module>ccweb-mail</module>
        <module>ccweb-jobs</module>
        <module>ccweb-plugin</module>
        <module>ccweb-clients</module>
    </modules>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.2.6.RELEASE</version>
        <relativePath /> <!-- lookup parent from repository -->
    </parent>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <docker.image.prefix>springboot</docker.image.prefix>
        <java.version>1.8</java.version>
        <github.global.server>github</github.global.server>
        <spring-cloud.version>Hoxton.RELEASE</spring-cloud.version>
        <logback-kafka-appender.version>0.2.0-RC1</logback-kafka-appender.version>
        <janino.version>3.0.8</janino.version>
    </properties>
    <repositories>
        <repository>
            <id>ali</id>
            <url>https://maven.aliyun.com/repository/central</url>
            <releases>
                <updatePolicy>always</updatePolicy>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <updatePolicy>always</updatePolicy>
                <enabled>true</enabled>
                <checksumPolicy>ignore</checksumPolicy>
            </snapshots>
        </repository>
        <repository>
            <id>ccweb</id>
            <url>https://gitee.com/ccait/ccweb/raw/release</url>
            <releases>
                <updatePolicy>always</updatePolicy>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <updatePolicy>always</updatePolicy>
                <enabled>true</enabled>
                <checksumPolicy>ignore</checksumPolicy>
            </snapshots>
        </repository>
    </repositories>


    <distributionManagement>
        <repository>
            <id>gitee</id>
            <url>file:${project.build.directory}/deploy</url>
        </repository>
    </distributionManagement>

